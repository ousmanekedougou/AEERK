{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-gd": "*",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "brian2694/laravel-toastr": "^5.56",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "informagenie/orange-sms": "^1.2",
        "intervention/image": "^2.5",
        "laravel/framework": "^7.24",
        "laravel/nexmo-notification-channel": "^2.5",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.4",
        "mercuryseries/flashy": "^1.0",
        "mike42/escpos-php": "^2.2",
        "nexmo/client-core": "^2.7",
        "nexmo/laravel": "^2.4",
        "nyholm/psr7": "^1.3",
        "paydunya/paydunya": "^1.0",
        "php-http/httplug": "^2.2",
        "php-http/message-factory": "^1.0",
        "symfony/http-client": "^5.2"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "paydunya/paydunya": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files":[
            "app/Helpers/helper.php",
            "app/Helpers/Sms.php",
            "app/Helpers/paydunya.php"
        ]
    },
    "autoload-dev": {
         "psr-4": {
            "Tests\\": "tests/"
        },
        "files":[
            "app/Helpers/helper.php",
            "app/Helpers/Sms.php",
            "app/Helpers/paydunya.php"
        ]
       
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [ 
            "ln -sr storage/app/public public/storage",
            "php artisan clear-compiled",
            "chmod -R 777 public/"
             ]
    }
}
